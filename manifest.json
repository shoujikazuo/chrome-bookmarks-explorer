{
    "name": "BookmarksExplorer",
    "version": "0.1",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self'; object-src 'self'; 'unsafe-inline'; ",
    "browser_action": {
      "default_title": "Ext title",
      "default_popup": "popup.html"
    },
    "permissions": [
      "bookmarks"
    ]
  }