{
    "name": "BookmarksExplorer",
    "version": "0.1",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://cdn.rawgit.com; object-src 'self'; 'unsafe-inline'; ",
    "browser_action": {
      "default_title": "Ext title",
      "default_popup": "popup.html"
    },
    "background": {
      "persistent": false,
      "scripts": [
        "./utils/array/arrayutils.js",
        "./model/maindriver.js",
        "./model/bookmarks/flatten.js",
        "./model/http-response/no-cache.js"
      ]
    },
    "permissions": [
      "http://*/*",
      "https://*/*",
      "bookmarks"
    ]
  }